							bacthInsertMap.add(valuesMap);
						}else{
							valuesMap=new HashMap<String, Object>();
							valuesMap.put("values", accessNumStr[i]);
							bacthInsertMap.add(valuesMap);
						}
						if(i!=0&&i%100==0){
							Params params=new Params();
