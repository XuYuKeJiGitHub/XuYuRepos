<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuyurepos.dao.manager.ImportTempDao">
	<cache></cache>
	<resultMap type="com.xuyurepos.entity.manager.ImportTemp" id="importTemp">
		<!-- column存数据库中的属性   property存javabean中数据-->
		<id column="ID" property="id"/>
		<result column="A" property="a"/>
		<result column="B" property="b"/>
		<result column="C" property="c"/>
		<result column="D" property="d"/>
		<result column="E" property="e"/>
		<result column="F" property="f"/>
		<result column="G" property="g"/>
		<result column="H" property="h"/>
	</resultMap>
    
    <update  id="truncateTable" >
           truncate table IMPORT_TEMP 
     </update >
     
     <insert id="insert" parameterType="com.xuyurepos.entity.manager.ImportTemp">
          insert into IMPORT_TEMP(A,B,C,D,E,F,G,H)
          values(
          #{a,javaType=String}
          ,#{b,javaType=String}
          ,#{c,javaType=String}
          ,#{d,javaType=String}
          ,#{e,javaType=String}
          ,#{f,javaType=String}
          ,#{g,javaType=String}
          ,#{h,javaType=String}
          )          
     </insert>
     
     <insert id="insertByBatch" parameterType="java.util.List">
        insert into IMPORT_TEMP (A,B,C,D,E,F,G,H)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (#{item.a,jdbcType=VARCHAR}
            ,#{item.b,jdbcType=VARCHAR}
            ,#{item.c,jdbcType=VARCHAR}
            ,#{item.d,jdbcType=VARCHAR}
            ,#{item.e,jdbcType=VARCHAR}
            ,#{item.f,jdbcType=VARCHAR}
            ,#{item.g,jdbcType=VARCHAR}
            ,#{item.h,jdbcType=VARCHAR}
            )
        </foreach>
    </insert>
     
</mapper>