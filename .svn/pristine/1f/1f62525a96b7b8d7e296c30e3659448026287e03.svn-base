<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ include file="../../base.jsp"%>
<!DOCTYPE html>
<head>
    <meta charset="UTF-8">
    <title>物联卡信息管理管理</title>
    <link type="text/css" rel="stylesheet" href="<%=request.getContextPath()%>/css/main.css">
</head>
<body>
<div class="easyui-layout" data-options="fit:true" id="useBox">
    <div data-options="region:'center',split:true">
        <!--表格查询-->
        <div class="tableFind">
            <p>
	            <label>接入号;</label><input type="text"/>
	            <label>物联卡状态;</label><input type="text"/>
	            <label>客户名称号;</label><input type="text"/>
            </p>
            <p>
	            <label>ICCID;</label><input type="text"/>
	            <label>套餐名称;</label><select  name="sex" class="easyui-combobox"><option>1GB年卡</oprtion></select>
	            <label>IMSI;</label><input type="text"/>
            </p>
            <p>
	            <label>开户日期;</label><input class="easyui-datebox" data-options="sharedCalendar:'#cc'" style="width:8%;"><input style="width:8%;" class="easyui-datebox" data-options="sharedCalendar:'#cc'">
	            <label>卡号备注;</label></label><input type="text"/>
	            <label>群组;</label><input type="text"/>
            </p>
            <p>
	            <label>激活日期;</label><input class="easyui-datebox" data-options="sharedCalendar:'#cc'" style="width:8%;"><input style="width:8%;" class="easyui-datebox" data-options="sharedCalendar:'#cc'">
	            <label>沉默期到期;</label><input class="easyui-datebox" data-options="sharedCalendar:'#cc'" style="width:8%;"><input style="width:8%;" class="easyui-datebox" data-options="sharedCalendar:'#cc'">
	            <label>到期时间;</label><input class="easyui-datebox" data-options="sharedCalendar:'#cc'" style="width:8%;"><input style="width:8%;" class="easyui-datebox" data-options="sharedCalendar:'#cc'">
            </p>
             <p>
	            <label>本月累计使用流量;</label><select  name="sex" class="easyui-combobox" style="width:8%;"><option>大于</oprtion><option>等于</oprtion><option>小于</oprtion></select><input style="width:8%;" type="text"/>
	            <label>本月累计使用短信;</label><select  name="sex" class="easyui-combobox" style="width:8%;" ><option>大于</oprtion><option>等于</oprtion><option>小于</oprtion></select><input style="width:8%;" type="text"/>
	            <label>余额;</label><select  name="sex" class="easyui-combobox" style="width:8%;"><option>大于</oprtion><option>等于</oprtion><option>小于</oprtion></select><input style="width:8%;" type="text"/>
	            
	            <a id="btn1" href="javascript:" class="easyui-linkbutton tableFindBut" data-options="iconCls:'icon-collapse'" onclick="obj.find()">&nbsp;</a>
	            <a id="btn" href="javascript:" class="easyui-linkbutton tableFindBut" data-options="iconCls:'icon-search'" onclick="obj.find()">查询</a>
	            <a id="zhbtn" href="javascript:" class="easyui-linkbutton tableFindBut" data-options="iconCls:'icon-redo'" onclick="obj.find()">重置</a>
	            
            </p>
        </div>
        <div id="cc" class="easyui-calendar"></div>
        <!--表格列表-->
        <div class="tableCon">
            <table id="table" class="tableStyle"></table>
            <div id="tabelBut">
                <div>
                    <a href="#" class="easyui-linkbutton" plain="true"  onclick="obj.addBox()">数据批量导入</a>
                    <a href="#" class="easyui-linkbutton" plain="true"  onclick="obj.edit()">导出</a>
                </div>
            </div>
        </div>
    </div>
</div>

<!--//新增弹出框-->
<div id="addBox" >
    <form id="addForm" method="post">
        <div class="formDiv"><input type="file" style="width: 80%;" class="easyui-validatebox" name="name" data-options="required:true"><span class="formSpan">*</span><a href="#" class="easyui-linkbutton" id="res" iconCls="icon-redo" onclick="obj.res()" >导入</a></div>
        <div class="forSubmint">
            <a href="#" class="easyui-linkbutton"  iconCls="icon-ok"  onclick="obj.sum()">模板下载</a>
        </div>

    </form>
</div>
<script type="text/javascript">
var bathPath="<%=basePath%>";
</script>
<script src=iccidManager.js></script>


</body>
</html>