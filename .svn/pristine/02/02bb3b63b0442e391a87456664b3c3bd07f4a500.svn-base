<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE mapper  
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xuyurepos.dao.system.SystemAnnexeDao">
	<cache></cache>
	<resultMap type="com.xuyurepos.entity.system.SystemAnnexe" id="systemAnnexe">
		<!-- column存数据库中的属性   property存javabean中数据-->
		<id column="ANNEXE_ID" property="annexeId"/>
		<result column="CREATE_TIME" property="createTime"/>
		<result column="ANNEXE_TYPE" property="annexeType"/>
		<result column="ANNEXE_SIZE" property="annexeSize"/>
		<result column="RELATION_INFO" property="relationInfo"/>
		<result column="RELATION_MOD" property="relationMod"/>
		<result column="UPLOAD_USER" property="uploadUser"/>
		<result column="UPLOAD_USER_NAME" property="uploadUserName"/>
		<result column="UPLOAD_PATH" property="uploadPath"/>
		<result column="UPLOAD_BATCHNO" property="uploadBatchno"/>
	</resultMap>
	
	 <select id="getList" parameterType="com.xuyurepos.entity.system.SystemAnnexe" resultMap="systemAnnexe">
        SELECT * FROM SYSTEM_ANNEXE where 1=1
        <if test="uploadBatchno != null and uploadBatchno != ''">
                    AND UPLOAD_BATCHNO=#{uploadBatchno}
        </if>
    </select>
</mapper>