        // 停复机
        del:function () {
               var rows=$("#table").datagrid("getSelections");
               if(rows.length>0){
                       $.messager.confirm('信息提示','你确定要停复机吗？',function (flg) {
                               if(flg){
                                       var ids=[];
                                       for(i=0;i<rows.length;i++){
                                               ids.push(rows[i].accessNum);

                                       }
                                       var num=ids.length;
                                      $.ajax({
                                              type:'post',
                                              url:bathPath+'/iccidmanager/changeCardStateAll',
                                              data:{
                                            	  accessNums:ids.join(';')
                                              },
                                              beforeSend: function () {
                                                  $.messager.progress({
                                                      title: '提示信息',
                                                      msg: '正在处理，请稍候……',
                                                      text: ''
                                                  });
                                              },
                                              complete: function () {
                                                  $.messager.progress('close');
                                              },
                                              beforesend:function () {
                                                      $("#table").datagrid('loading');
                                                      
                                              },
                                              success:function (data) {
                                                      if(data){
                                                              $("#table").datagrid('loaded');
                                                              $("#table").datagrid('load');
                                                              $("#table").datagrid('unselectAll');
                                                              $.messager.show({
                                                                      title:'提示',
                                                                      msg:num+'个用户被停复机'
                                                              })
                                                      }
                                                      else{
                                                              $.messager.show({
                                                                      title:'警示信息',
                                                                      msg:"停复机失败"
                                                              })
                                                      }
                                              }
                                      })
                               }

                       })

               }
               else{
                       $.messager.alert('信息提示','请选择要停复机的记录','info');
               }

        },
